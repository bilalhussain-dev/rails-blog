
  <% if obj.errors.any? %>
    <div class="alert alert-danger">
      <h3 class="h3">Sorry! Before submit, Fix the errors below</h3>
      <ul class="list-unstyled">
        <% obj.errors.full_messages.each do |err, count| %>
          <li>*<%= err %></li>
        <%end %>
      </ul>
    </div>
  <%end %>
